programa {
  funcao inicio() {
    inteiro num1, num2

    escreva ("informe o Primeiro Valor:")
    leia (num1)

    escreva ("informe o Segundo Valor:")
    leia (num2)

    se (num1+num2 >= 10)
      escreva("A soma entre eles é: ", num1+num2)

    senao
      escreva("A soma entre eles é menor que 10 :c")

  }
}
